                     <valItem ident="participle" mode="add">
                        <desc>The value of the this gram-type can be "present|past"</desc>
                     </valItem>
                     <valItem ident="arguments" mode="add">
                        <desc>arguments to a verb</desc>
                     </valItem>
                     <valItem ident="state" mode="add">
                        <desc>construct</desc>
                     </valItem>
                     <!--
                     <valItem ident="morphType">
                        <desc>mophological type</desc>
                        <remarks>
